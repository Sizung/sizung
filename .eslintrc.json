{
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
      "mocha": true
  },
  "plugins": [
    "babel",
    "react"
  ],
  "rules": {
    "max-len": 0,
    "prefer-template": 0,
    "arrow-body-style": 0,
    "consistent-return": 0,
    "new-cap": 0,
    "babel/new-cap": 1,
    "react/prop-types": 0,
    "no-multi-spaces": 0,
    "react/jsx-indent-props": 0
  }
}
